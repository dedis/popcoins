<Page
        class="page"
        loaded="onLoaded">

    <ActionBar class="action-bar">
        <Label class="action-bar-title" text="Conode QR Stats"/>
    </ActionBar>

    <GridLayout class="page-content" rows="*,auto" columns="*">
        <Image id="image-conode-stats" stretch="aspectFit" row="0" tap="copyToClipboard"/>
        <Label class="label-title" id="label-conode-stats" row="1" textWrap="true" textAlignment="left"/>
    </GridLayout>

</Page>

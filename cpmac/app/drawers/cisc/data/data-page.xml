<Page
        class="page">

    <ActionBar class="action-bar">
        <Label class="action-bar-title" text="Organizer"/>
    </ActionBar>

    <StackLayout class="page-content" orientation="vertical">
        <Label class="label-title" text="Key Pairs"/>

        <Button text="Create Key pair" tap="keyPairButtonTapped"/>

        <Label class="label-title" text="Propose vote" row="4" colSpan="3"/>
        <Button text="Propose the new KeyPair" tap="proposeVoteButtonTapped"/>
    </StackLayout>

</Page>
